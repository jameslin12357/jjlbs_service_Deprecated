        function getBuildingNumber(str){
    var r = str.match(/[a-zA-Z]/);
    if (r !== null) {
        return str.slice(r["index"]);
    } else {
        var r2 = str.match(/\d/);
        if (r2 !== null) {
            return str.slice(r2["index"]);
        } else {
            return str;
        }
    }
}

